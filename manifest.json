{
    "manifest_version": 2,
    "name": "Strava Leaderboards",
    "description": "get segment's leaderboards",
    "version": "0.1",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery-2.2.4.min.js", "content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "permissions": ["tabs"]
  }
